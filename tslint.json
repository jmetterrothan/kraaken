{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "double", "jsx-double"],
    "no-console": false,
    "no-empty": [true, "allow-empty-catch", "allow-empty-functions"],
    "no-bitwise": false,
    "max-line-length": [true, 400],
    "object-literal-sort-keys": false,
    "no-namespace": false
  }
}
